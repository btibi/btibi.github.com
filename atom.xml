---
layout: nil
title : Atom Feed
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Java Blog</title>
  <link href="http://btibi.github.com/"/>
  <link type="application/atom+xml" rel="self" href="http://example.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://btibi.github.com/</id>
  <author>
    <name>Tibor Baranyi</name>
    <email>baranyitibi@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://btibi.github.com{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://btibi.github.com/{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Tibor Baranyi</name>
      <uri>http://btibi.github.com/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>